{
  "expo": {
    "name": "phodos",
    "slug": "phodos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Phodos to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Phodos to save photos."
      },
      "bundleIdentifier": "com.whipchatterly404.phodos"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.whipchatterly404.phodos",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3d88860f-70ee-4b76-9589-a3dfc570b258"
      }
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Phodos to access your photos.",
          "savePhotosPermission": "Allow Phodos to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "owner": "whipchatterly404"
  }
}
